---
title: Archive
layout: base.njk
tags: pages
eleventyExcludeFromCollections: true
---

<h1>{{ title }}</h1>

<ol>
  {%- for post in collections.all | reverse %}
    <li>
      <a href="{{ post.url }}">{{ post.data.title }}</a>- 
      <time datetime="{{ post.data.date | htmlDateString }}">{{ post.data.date | readableDate}}</time>
    </li>
  {%- endfor %}
</ol>