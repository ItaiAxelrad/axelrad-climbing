---
title: Tags
stylesheet: taglist
permalink: /tags/
eleventyExcludeFromCollections: true
layout: page.njk
---
<h1>{{ title }}</h1>
{% for tag in collections.tagList | excludeTags | sort(arr) %}
  {% set tagUrl %}/tags/{{ tag }}{% endset %}
  <a href="/tags/{{ tag | replace("'", "") | slug | url }}" class="tag p-category" rel="tag">{{ tag }}</a>
{% endfor %}